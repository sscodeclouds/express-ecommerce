<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.items.length > 0) { %>
                <ul>
                    <% orders.items.forEach((product, index) => { %>
                        <li><%= product.title + ' (' + product.quantity + ')' %></li>
                    <% }) %>
                </ul>
            <% } else { %>
                <h1>No order found</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>